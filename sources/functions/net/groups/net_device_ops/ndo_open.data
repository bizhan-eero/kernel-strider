[group]
	# Name of the target function
	function.name = ndo_open

	is_rtnl_locked = yes

	code.pre =>>
		/* Relation w.r.t. ndo_poll_controller */
		kedr_happens_after(ls->tid, pc, (unsigned long)dev + 2);
	<<

	code.post =>>
		/* Relation w.r.t. ndo_poll_controller */
		kedr_happens_before(ls->tid, pc, (unsigned long)dev + 3);
	<<
